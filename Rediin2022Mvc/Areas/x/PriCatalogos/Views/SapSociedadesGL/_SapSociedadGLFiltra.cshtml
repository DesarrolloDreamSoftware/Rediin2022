@model ESapSociedadGLFiltro

<form asp-action="_FiltraSapSociedadGL">
    <div mpanel="Base" xpnl-rules="ViewBag.Reglas">
        <div melemento="CapLin">
            <div melemento="FilCol" melem-cols="4">
                <input mval-filter="Model" xval-for="FilSapSociedadGLNombre"/>
            </div>
            <div melemento="FilCol" melem-cols="4">
                <select mval-filter="Model" xval-for="FilActivo"></select>
            </div>
            <div melemento="FilColBtn" melem-cols="4">
                <button maccion="Filtra" asp-action="@nameof(SapSociedadesGLController.SapSociedadGLFiltra)"></button>
                <button maccion="LimpiaFiltros" asp-action="@nameof(SapSociedadesGLController.SapSociedadGLLimpiaFiltros)"></button>
            </div>
        </div>
    </div>
</form>
